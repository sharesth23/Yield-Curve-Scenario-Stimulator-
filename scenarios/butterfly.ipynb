   "source": [
    "def butterfly(yields, maturities, bp=20):\n",
    "    center = maturities.mean()\n",
    "    shock = -((maturities - center) ** 2) / center**2\n",
    "    shock = shock / shock.min() * (bp / 10000)\n",
    "    return yields + shock\n"
   ]
