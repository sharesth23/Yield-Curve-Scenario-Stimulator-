    "def historical_shocks(df):\n",
    "    pivot = df.pivot(index=\"date\", columns=\"maturity\", values=\"yield\")\n",
    "    returns = pivot.diff().dropna()\n",
    "    return returns.quantile([0.05, 0.5, 0.95])\n"
   ]
