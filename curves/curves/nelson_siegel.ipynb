   "source": [
    "def nelson_siegel(t, beta0, beta1, beta2, tau):\n",
    "    return (\n",
    "        beta0\n",
    "        + beta1 * (1 - np.exp(-t / tau)) / (t / tau)\n",
    "        + beta2 * ((1 - np.exp(-t / tau)) / (t / tau) - np.exp(-t / tau))\n",
    "    )"
   ]
