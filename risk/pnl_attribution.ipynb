    "def pnl_attribution(dv01, convexity, dy):\n",
    "    level = dv01 * dy\n",
    "    curvature = 0.5 * convexity * dy**2\n",
    "    return {\n",
    "        \"level_pnl\": level,\n",
    "        \"convexity_pnl\": curvature,\n",
    "        \"total_pnl\": level + curvature\n",
    "    }\n",
    "\n"
